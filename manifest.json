{
    "manifest_version": 3,
    "name": "discopfp",
    "version": "1.0",
    "description": "Preview Discord profile pictures in various resolutions.",
    "permissions": ["scripting", "activeTab", "storage"],
    "host_permissions": ["*://discord.com/*"],
    "action": {
      "default_title": "discopfp"
    },
    "background": {
      "service_worker": "src/js/background.js"
    },
    "icons": {
      "128": "src/icons/icon.jpg"
    }
  }
  